{
    "name": "djammix/ebot",
    "description": "Laravel package for Telegram Bot API integration",
    "type": "library",
    "license": "MIT",
    "keywords": ["telegram", "bot", "laravel", "api", "integration"],
    "authors": [
        {
            "name": "DJamMix",
            "email": "antropov.viktor1234@yandex.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "djammix\\EBot\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "djammix\\EBot\\EBotServiceProvider"
            ]
        }
    },
    "suggest": {
        "ext-curl": "For better HTTP request handling",
        "laravel/framework": "For full Laravel integration"
    },
    "prefer-stable": true
}